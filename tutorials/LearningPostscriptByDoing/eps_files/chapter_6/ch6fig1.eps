%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 0 0 425 125
%%Pages: 1
/u 20 def % 20 point per unit
/lw1 {0.5 u div} def % default linewidth
/lw2 {0.7 u div} def % default linewidth
/lw3 {1 u div} def % default linewidth
%
/arrowhead {% stack: s x1 y1
            % current point: x0 y0
  gsave
    currentpoint   % s x1 y1 x0 y0
    4 2 roll exch  % s x0 y0 y1 x1
    4 -1 roll exch % s y0 y1 x0 x1
    sub 3 1 roll   % s x1-x2 y0 y1
    sub exch       % s y0-y1 x1-x2
  % rotate over arctan((y0-y1)/(x1-x2))
    atan rotate    % s 
    dup scale      % scale by s
    -7 2 rlineto   % draw arrowhead
    1 -2 rlineto
    -1 -2 rlineto
   closepath fill  % fill arrowhead
 grestore
 newpath
} def
%
/grid {4 dict begin % m x n grid
   /n exch def
   /m exch def
   lw1 setlinewidth
   0 1 n 1 sub {/i exch def
     0 1 m 1 sub {/j exch def
       newpath
       gsave 
         .88235 0.97647 0.97647 setrgbcolor 
         i j 1 1 rectfill
       grestore
       .36040 0.97647 0.97647  setrgbcolor 
       i j 1 1 rectstroke
    } for
  } for
end } bind def
%
/coordsys {4 dict begin % m x n grid
   /n exch def
   /m exch def
   /y exch def
   /x exch def
   newpath
   lw2 setlinewidth
   0 setgray 
   x y 1 sub moveto
   0 m 1 add rlineto
   x 1 sub y moveto
   n 1 add 0 rlineto
   stroke
   lw3 setlinewidth
   newpath
   1 0 0 setrgbcolor
   x y moveto
   0 1 rlineto
   stroke
   newpath x y 1 add moveto 0.08 x y arrowhead   
   0.7490 0.4 0.9843 setrgbcolor
   x y moveto
   1 0 rlineto
   stroke
   newpath x 1 add y moveto 0.08 x y arrowhead   
end } bind def
%%EndProlog
