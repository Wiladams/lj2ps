%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 0 0 104 104
%%BeginProlog
/n 6 def % how many iterations 
/s {n 0 ne {100 2 n exp 1 sub div}{100} ifelse} def
/lw 0.8 s div def
% reflect about line from (0,0) to (1,-1)
/R {90 rotate -1 1 scale} bind def
/invR { -1 1 scale -90 rotate} bind def
% reflect about line from (0,0) to (1,1)
/M {-1 1 scale 90 rotate} def
/invM { -90 rotate -1 1 scale } def
/H {
  dup 0 gt {
    1 sub
    M H invM 1 0 rlineto
    H 0 1 rlineto
    H -1 0 rlineto
    R H invR
    1 add
  } if
} bind def
%%EndProlog
2 2 translate
s dup scale
lw setlinewidth
1 setlinecap
newpath
0 0 moveto 0 0 rlineto
n H
stroke
showpage
%%EOF
