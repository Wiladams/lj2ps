%!PS-Adobe-2.0 EPSF-2.0
%%BoundingBox: 0 0 240 240

%%%%%%% 

2 setlinejoin

120 120 translate
%180 300 translate
%2 2 scale


/R 118 def
/f 1 def     %%%% vergrotingsfactor eutactische ster
/lw1 .8 def   %%%% lijndikte
/lw2 1 def
/g 1 def
/d 0.4 def   %%%% grijsaanpassingsfactor


/c5 72 cos def
/s5 72 sin def

%%%%%%%% projectiehoeken:
/alpha 4 def
/beta -12 def

%%%%%%%% lichtrichtinghoeken:
/alpha1 47 def
/beta1 -40 def


%%%%%%%%% tangens
/tan {
  /a exch def
  a sin a cos div 
} bind def

%%%%%%%%% geprojecteerde basisvectoren:

/xa alpha sin def
/ya alpha cos beta sin mul def

/xb alpha cos def
/yb alpha sin beta sin neg mul def

/xc 0 def
/yc beta cos def

%%%%%%%%% orthogonale projectie:

/coord {
/z exch def 
/y exch def
/x exch def
x xa mul y xb mul add z xc mul add 
x ya mul y yb mul add z yc mul add
} bind def

/norm {
/n3 exch def
/n2 exch def
/n1 exch def
/nm n1 n1 mul n2 n2 mul add n3 n3 mul add sqrt def
n1 alpha cos mul beta cos mul 
n2 alpha sin mul neg beta cos mul add 
n3 beta sin mul neg add nm div} bind def 

/norm1 {
/n3 exch def
/n2 exch def
/n1 exch def
/nm n1 n1 mul n2 n2 mul add n3 n3 mul add sqrt def
n1 alpha1 cos mul beta1 cos mul 
n2 alpha1 sin mul neg beta1 cos mul add 
n3 beta1 sin mul neg add nm div} bind def 



%%%%%%%%% cosinus hoek vector (n1,n2,n3) 
%%%%%%%%% met projectierichting

/projCos {
/n3 exch def
/n2 exch def
/n1 exch def
/nm n1 n1 mul n2 n2 mul add n3 n3 mul add sqrt def
n1 alpha cos mul beta cos mul 
n2 alpha sin mul neg beta cos mul add 
n3 beta sin mul neg add nm div} bind def 


%%%%%%%%% cosinus hoek vector (n1,n2,n3) 
%%%%%%%%% met lichtrichting

/lichtCos {
/nn3 exch def
/nn2 exch def
/nn1 exch def
/nnm nn1 nn1 mul nn2 nn2 mul add nn3 nn3 mul add sqrt def
nn1 alpha1 cos mul beta1 cos mul 
nn2 alpha1 sin mul neg beta1 cos mul add 
nn3 beta1 sin mul neg add nnm div} bind def 


%%%%%%%%% cross product (aa1,aa2,aa3)X(bb1,bb2,bb3)
/cp {
/bb3 exch def
/bb2 exch def
/bb1 exch def
/aa3 exch def
/aa2 exch def
/aa1 exch def
aa2 bb3 mul aa3 bb2 mul sub
aa3 bb1 mul aa1 bb3 mul sub
aa1 bb2 mul aa2 bb1 mul sub
} bind def


%%%%%%%%% tekenen van een driehoek 
%%%%%%%%% hoekpunten: a1 a2 a3 b1 b2 b3 c1 c2 c3
/driehoek {
/c3 exch def  /c2 exch def  /c1 exch def
/b3 exch def  /b2 exch def  /b1 exch def
/a3 exch def  /a2 exch def  /a1 exch def
/bc1 b1 c1 add 2 div def
/bc2 b2 c2 add 2 div def
/bc3 b3 c3 add 2 div def
/ca1 a1 c1 add 2 div def
/ca2 a2 c2 add 2 div def
/ca3 a3 c3 add 2 div def
/ab1 b1 a1 add 2 div def
/ab2 b2 a2 add 2 div def
/ab3 b3 a3 add 2 div def

/proj 
  c1 a1 sub c2 a2 sub c3 a3 sub b1 a1 sub b2 a2 sub b3 a3 sub 
  cp projCos neg def
/kleur1 
  c1 a1 sub c2 a2 sub c3 a3 sub b1 a1 sub b2 a2 sub b3 a3 sub 
  cp lichtCos neg def
/kleur 1 d sub kleur1 d mul add def
proj 0 gt {
  newpath
  a1 a2 a3 coord moveto
  b1 b2 b3 coord lineto
  c1 c2 c3 coord lineto
  a1 a2 a3 coord lineto
  ca1 ca2 ca3 coord lineto
  bc1 bc2 bc3 coord lineto
  ab1 ab2 ab3 coord lineto
  ca1 ca2 ca3 coord lineto
  closepath
  gsave kleur setgray fill grestore 
  stroke
  } if
} bind def

/driehoek1 {
/c3 exch def  /c2 exch def  /c1 exch def
/b3 exch def  /b2 exch def  /b1 exch def
/a3 exch def  /a2 exch def  /a1 exch def
/bc1 b1 c1 add 2 div def
/bc2 b2 c2 add 2 div def
/bc3 b3 c3 add 2 div def
/ca1 a1 c1 add 2 div def
/ca2 a2 c2 add 2 div def
/ca3 a3 c3 add 2 div def
/ab1 b1 a1 add 2 div def
/ab2 b2 a2 add 2 div def
/ab3 b3 a3 add 2 div def

/proj 
  c1 a1 sub c2 a2 sub c3 a3 sub b1 a1 sub b2 a2 sub b3 a3 sub 
  cp projCos neg def
/kleur1 
  c1 a1 sub c2 a2 sub c3 a3 sub b1 a1 sub b2 a2 sub b3 a3 sub 
  cp lichtCos neg def
/kleur 1 d sub kleur1 d mul add def
proj 0.15 gt {
  newpath
  a1 a2 a3 coord moveto
  b1 b2 b3 coord lineto
  c1 c2 c3 coord lineto
  a1 a2 a3 coord lineto
  ca1 ca2 ca3 coord lineto
  bc1 bc2 bc3 coord lineto
  ab1 ab2 ab3 coord lineto
  ca1 ca2 ca3 coord lineto
  closepath
  gsave kleur setgray fill grestore 
  stroke
  } if
} bind def


/driehoekje {
/c3 exch def  /c2 exch def  /c1 exch def
/b3 exch def  /b2 exch def  /b1 exch def
/a3 exch def  /a2 exch def  /a1 exch def
/bc1 b1 c1 add 2 div def
/bc2 b2 c2 add 2 div def
/bc3 b3 c3 add 2 div def

/proj 
  c1 a1 sub c2 a2 sub c3 a3 sub b1 a1 sub b2 a2 sub b3 a3 sub 
  cp projCos neg def
/kleur1 
  c1 a1 sub c2 a2 sub c3 a3 sub b1 a1 sub b2 a2 sub b3 a3 sub 
  cp lichtCos neg def
/kleur 1 d sub kleur1 d mul add def
proj 0 gt {
  newpath
  a1 a2 a3 coord moveto
  b1 b2 b3 coord lineto
  c1 c2 c3 coord lineto
  closepath
  gsave kleur setgray fill grestore 
  stroke
  } if
} bind def



%%%%%%%%% rotatie over 72 graden om z-as

/rot5z {
/z exch def 
/y exch def
/x exch def
x c5 mul y s5 mul sub 
x s5 mul y c5 mul add
z
} bind def

%%%%%%%%% rotatie over a graden om z-as

/rotaz {
/ca a cos def
/sa a sin def
/z exch def 
/y exch def
/x exch def
x ca mul y sa mul sub 
x sa mul y ca mul add
z
} bind def

%%%%%%% phi: halve hoek tussen twee 5-assen

/phi 2 1 atan 2 div def 
/c phi 2 mul cos def
/s phi 2 mul sin def

%%%%%%%%% spiegeling in bissectricevlak van twee 5-assen
%%%%%%%%% (de z-as en as in het xz-vlak)

/r2p {
/z exch def 
/y exch def
/x exch def
x c mul neg z s mul add 
y
x s mul z c mul add
} bind def

%%%%%%% basisvectoren

0 0 R %%%%%% langs z-as
/w0 exch def
/v0 exch def
/u0 exch def

0 0 R r2p  %%%%%% in xz-vlak
/w1 exch def
/v1 exch def
/u1 exch def

0 0 R r2p rot5z
/w2 exch def
/v2 exch def
/u2 exch def

0 0 R r2p rot5z rot5z
/w3 exch def
/v3 exch def
/u3 exch def

0 0 R r2p rot5z rot5z rot5z
/w4 exch def
/v4 exch def
/u4 exch def

0 0 R r2p rot5z rot5z rot5z rot5z
/w5 exch def
/v5 exch def
/u5 exch def

/vert [%
       [u1 v1 w1] [u1 neg v1 neg w1 neg] 
       [u2 v2 w2] [u2 neg v2 neg w2 neg]
       [u3 v3 w3] [u3 neg v3 neg w3 neg]
       [u4 v4 w4] [u4 neg v4 neg w4 neg]
       [u5 v5 w5] [u5 neg v5 neg w5 neg]] def

