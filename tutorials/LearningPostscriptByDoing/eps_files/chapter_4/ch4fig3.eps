%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 0 0 180 60
%%BeginProlog
/Courier findfont 8 scalefont setfont 
/line { 
  newpath 0 0 moveto 72 0 lineto stroke
} def
%%EndProlog
10 50 translate
10 setlinewidth 0 setlinecap 0 setgray line 
1 setlinewidth 0.8 setgray line
0 setgray 85 -2 moveto (butt:    linecap=0) show

0 -20 translate
10 setlinewidth 1 setlinecap 0 setgray line
1 setlinewidth 0.8 setgray line
0 setgray 85 -2 moveto (rounded: linecap=1) show

0 -20 translate
10 setlinewidth 2 setlinecap 0 setgray line
1 setlinewidth 0.8 setgray line
0 setgray 85 -2 moveto (squared: linecap=2) show
showpage
%%EOF
