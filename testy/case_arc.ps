%!
300 300 translate
%20 20 scale
%0.2 setlinewidth


% draw some axis lines
newpath
1 0 0 setrgbcolor
0 0 moveto
200 200 lineto
stroke

newpath
0 1 0 setrgbcolor
0 0 moveto
200 0 lineto
stroke

newpath
0 0 1 setrgbcolor
0 0 moveto
0 200 lineto
stroke 

% now draw some arcs
newpath
0 setgray
0 0 100 30 45 arc
%closepath
stroke

showpage